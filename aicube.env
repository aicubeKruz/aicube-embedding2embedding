# AICUBE Embedding2Embedding API Environment Configuration

# Application Settings
API_NAME=aicube-embedding2embedding
API_VERSION=v1
ENVIRONMENT=development
DEBUG=true

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=./aicube-logs/aicube-embedding2embedding.log

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8080", "http://127.0.0.1:3000"]

# Model Configuration
MODELS_PATH=./aicube-models
MAX_EMBEDDING_DIMENSION=4096
MAX_BATCH_SIZE=32

# Performance Configuration
DEVICE=cpu
MAX_WORKERS=4

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Cache Configuration
ENABLE_MODEL_CACHE=true
CACHE_TTL=3600

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=8001

# Timeouts
REQUEST_TIMEOUT=30
MODEL_LOAD_TIMEOUT=120

# AICUBE Technology
AICUBE_TECHNOLOGY_NAME=AICUBE TECHNOLOGY
AICUBE_MODELS=["FastAPI Framework", "PyTorch Deep Learning", "MLP Neural Networks", "Advanced ML Algorithms"]